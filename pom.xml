			<name>CET snapshot Repo</name>
			<url>http://source.cet.uct.ac.za/nexus/content/repositories/snapshots</url>
			<layout>default</layout>
		</snapshotRepository>
	</distributionManagement>
	<scm>
		<connection>scm:svn:http://source.cet.uct.ac.za/svn/sakai/AccountInfo/branches/AccountInfo-1.7.x</connection>
		<developerConnection>scm:svn:http://source.cet.uct.ac.za/svn/sakai/AccountInfo/branches/AccountInfo-1.7.x</developerConnection>
		<url>http://source.cet.uct.ac.za/svn/sakai/AccountInfo/branches/AccountInfo-1.7.x</url>
	</scm>

	<repositories>
		<repository>
			<id>sakai-maven</id>
			<name>Sakai Maven Repo</name>
			<layout>default</layout>
			<url>http://source.sakaiproject.org/maven2</url>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
		</repository>
	</repositories>



</project>
