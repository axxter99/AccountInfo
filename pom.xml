	<name>UCT Account info tool</name>
	<groupId>org.sakaiproject.accountinfo</groupId>
	<artifactId>accountinfo</artifactId>
	<packaging>pom</packaging>
	<version>1.7-SNAPSHOT</version>
	<modules>
		<module>tool</module>
	</modules>
	<distributionManagement>
		<repository>
			<uniqueVersion>false</uniqueVersion>
			<id>release</id>
			<name>CET Release Repo</name>
			<url>http://source.cet.uct.ac.za/nexus/content/repositories/releases</url>
			<layout>default</layout>
		</repository>
		<snapshotRepository>
			<uniqueVersion>false</uniqueVersion>
